#
# add -Dlog4j.configuration=org/argouml/resource/notation.lcf
# to the command line to use this log4j configuration
#
# $Id$
#
log4j.rootCategory=ERROR, NOTATION
#
log4j.category.org.argouml.application.notation=DEBUG
# 
log4j.appender.NOTATION=org.apache.log4j.ConsoleAppender
log4j.appender.NOTATION.layout=org.apache.log4j.PatternLayout
log4j.appender.NOTATION.layout.ConversionPattern=%5p: %m (%F:%L)%n
log4j.appender.NOTATION.Threshold=DEBUG
