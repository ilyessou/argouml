<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for launching ArgoUML with WebStart -->
<jnlp spec="1.0+"
      codebase="http://argouml.tigris.org/nonav/download/jws/"
      href="http://argouml.tigris.org/nonav/download/jws/ArgoUMLWebStart.jnlp">
  <information>
    <title>ArgoUML 0.10</title>
    <vendor>ArgoUML Open Source development group (hosted at Tigris.org)</vendor>
    <homepage href="http://argouml.tigris.org/"/>
    <description>ArgoUML 0.10</description>
    <description kind="short">ArgoUML, a Swing based editor. Version 0.10.</description>
    <icon href="images/notepad.gif"/>
