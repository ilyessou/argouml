    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/xerces.jar"/>
    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/antlrall.jar"/>
    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/gef.jar"/>
    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/ocl-argo.jar"/>
    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/nsuml.jar"/>
    <jar href="http://argouml.tigris.org/files/documents/4/0/0100/log4j.jar"/>
  </resources>
  <application-desc main-class="org.argouml.application.Main"/>
</jnlp>
