#
# add -Dlog4j.configuration=org/argouml/resource/default_console.lcf
# to the command line to use this log4j configuration
#
log4j.rootCategory=WARN, DEFAULTlog4j.category.org.argouml=WARN
# 
log4j.appender.DEFAULT=org.apache.log4j.ConsoleAppender
log4j.appender.DEFAULT.layout=org.apache.log4j.PatternLayout
log4j.appender.DEFAULT.layout.ConversionPattern=%5p: %m (%F:%L)%n
log4j.appender.DEFAULT.Threshold=WARN
