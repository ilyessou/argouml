/*
  The LETTER and DIGIT rules are generated from the Unicode Consortium's
  online character database using the following Python program.  These sets
  should be relatively stable, but the program can be used to regenerate
  them if necessary
  ---------------
  
import urllib

def isLetter(candidate):
    """Return true if character is in one of the letter categories"""
    category = candidate.split(";")[2]
    return category in ["Lu","Ll","Lt","Lo"]

def isDigit(candidate):
    """Return true if character is in one of the digit categories"""
    category = candidate.split(";")[2]
    return category in ["Nd"]

def computeRanges(codeList):
    """Compute a list of decimal ranges from a list of hex number strings"""
    decimalCodes = [int(code,16) for code in codeList]
    rangeStart = decimalCodes[0]
    lastCode = decimalCodes[0]
    ranges = []
    
    for code in decimalCodes :
        if code - lastCode > 1 :
            ranges.append([rangeStart, lastCode])
            rangeStart = code
        lastCode = code
    return ranges

def printRanges(rangeList):
    for range in rangeList :
        if range[0] < 65536 :
            if range[1] > 65535 :
                range[1] = 65535
            if range[0] == range[1] :
                print "        '\u%04x'|" % (range[0])
            else :
                print "        '\u%04x'..'\u%04x'|" % (range[0],range[1])

def fetchPage(url) :
    socket = urllib.urlopen(url)
    page = socket.read()
    socket.close()
    return page.splitlines()

lines = fetchPage("http://www.unicode.org/Public/5.0.0/ucd/UnicodeData.txt")

print "protected" 
print "LETTER"
print "    :"
printRanges(computeRanges([line.split(";")[0] for line in lines if isLetter(line)]))
print "    ;"
print " "

print "protected" 
print "DIGIT"
print "    :"
printRanges(computeRanges([line.split(";")[0] for line in lines if isDigit(line)]))
print "    ;"
print " "
  
  ---------------
  */
  
