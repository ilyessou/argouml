// the basic element of an expression
primaryExpression
    :    identPrimary ( options {greedy=true;} : DOT "class" )?
    |    constant
    |    "true"
    |    "false"
    |    "null"
    |    newExpression
    |    "this"
    |    "super"
    |    LPAREN assignmentExpression RPAREN
        // look for int.class and int[].class
    |    builtInType
        ( LBRACK RBRACK )*
        DOT "class"
